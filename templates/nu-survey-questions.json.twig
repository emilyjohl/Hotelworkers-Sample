                    "union_comparison_value": "responses.mergeVars.wageRate",
                    "union_response_IWA": "{{ '${(responses.mergeVars.county == \'New York\') ? (\'<p>Under our master contract in your area, a \' + responses.classification + \' typically starts at <strong>\' + currency(responses.mergeVars.wageRate).multiply(0.75).format() + \' per hour</strong> \' + (responses.mergeVars.classification.tipped ? \'(before tips)\' : \'\') + \' and within several years, makes their way up to the full rate, currently <strong>\' + currency(responses.mergeVars.wageRate).format() + \' per hour</strong> \' + (responses.mergeVars.classification.tipped ? \'(before tips).\' : \'.\') + \'</p>\') : (\'<p>Under our master contract in your area, a \' + responses.classification + \' typically starts at <strong>\' + currency(responses.mergeVars.wageRate).multiply(0.75).format() + \'</strong> per hour\' + (responses.mergeVars.classification.tipped ? \' (before tips)\' : \'\') + \'.</p>\')}'| t | raw }}",
                    "union_response_GRIWA": "{{ '<p>Under our master contract in your area, a union-represented ${responses.classification} typically makes <strong>${currency(responses.mergeVars.wageRate).format()} per hour</strong>${responses.mergeVars.classification.tipped ? \' (before tips).\' : \'.\'}</p>'| t | raw }}"
               }
          ]
