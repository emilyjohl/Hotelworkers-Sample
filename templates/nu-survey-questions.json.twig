                    "subquestion": false,
                    "handle": "yearly_raises",
                    "label": "{{ 'Do you receive yearly raises of ${currency(responses.mergeVars.wageItemRates[\'Yearly Increase \' + (responses.mergeVars.classification.tipped ? \'(Tipped)\' : \'(NonTipped)\')]).format()} an hour or more?' | t | raw }}",
